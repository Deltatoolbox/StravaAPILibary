{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/api/**.yml",
          "docs/api/index.md"
        ]
      },
      {
        "files": [
          "docs/articles/**.md",
          "docs/articles/**/toc.yml",
          "docs/toc.yml",
          "docs/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docs/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/_site/**"
        ]
      }
    ],
    "dest": "docs/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appName": "StravaAPILibary",
      "_appTitle": "StravaAPILibary Documentation",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_enableContribution": true,
      "_lang": "en-us",
      "_disableContribution": false,
      "_disableAffix": false,
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableNav": false,
      "_disableRss": false,
      "_disableSitemap": false,
      "_disableSearch": false,
      "_disableSideFilter": false,
      "_disableFooter": false,
      "_rootPath": "",
      "_rel": "",
      "_lang": "en-us",
      "_tocPath": "",
      "_navPath": "",
      "_indexPath": "",
      "_sitePath": "",
      "_baseUrl": "",
      "_repoUrl": "https://github.com/your-repo/StravaAPILibary",
      "_docfxVersion": "2.75.1.0",
      "pdf": true
    },
    "fileMetadata": {
      "**/docs/api/**.yml": {
        "toc_rel": "~/docs/toc.yml"
      }
    }
  }
}
